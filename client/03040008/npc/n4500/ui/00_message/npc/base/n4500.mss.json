      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 27,
      "JumpGroupSerial" : 0,
      "Message" : "The essence of the \"Shield Sage\" is to become a\nfortress that protects one's comrades, and to be a\nreliable beacon that guides the course of battle.",
      "MsgSerial" : 397160,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
