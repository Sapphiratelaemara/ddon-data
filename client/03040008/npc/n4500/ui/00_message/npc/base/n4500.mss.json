    "GroupSerial" : 13964,
    "GroupType" : 43,
    "GroupTypeName" : "MSG_GROUP_TYPE_REGISTERD_ENTRY_TALK",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 6,
      "JumpGroupSerial" : 0,
      "Message" : "Oh, that's what you're after.\nAlright then.",
      "MsgSerial" : 255016,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 7,
      "JumpGroupSerial" : 0,
      "Message" : "I am Rudolfo.\nAs you can see, I am the lighthouse keeper.\nFormerly, I used to be an Instructor of the White\nKnights, teaching the ways of the Shield Sage.",
      "MsgSerial" : 255322,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 0
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 8,
      "JumpGroupSerial" : 0,
      "Message" : "Seems you've gained experience as a Shield Sage,\nand mastered the basics of protection.",
      "MsgSerial" : 255323,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 0
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 9,
      "JumpGroupSerial" : 0,
      "Message" : "With the Greatshield, you can intercept the\nenemy's attacks and convert them into the power to\ncounterattack--",
      "MsgSerial" : 255324,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 0
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 10,
      "JumpGroupSerial" : 0,
      "Message" : "And by drawing the enemy's attention to oneself,\nincrease the opportunities for your allies to\nattack.",
      "MsgSerial" : 255325,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 0
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 11,
      "JumpGroupSerial" : 0,
      "Message" : "You should already have grasped this.\nThat Greatshield is not only for your protection,\nbut is also a fortress for your comrades.",
      "MsgSerial" : 255326,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 0
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 12,
      "JumpGroupSerial" : 0,
      "Message" : "Your strength is already enough.\nHow about overcoming the trials I present and\nstriving for further improvement?",
      "MsgSerial" : 255465,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4500,
