      "DispType" : 0,
      "GmdIndex" : 25,
      "JumpGroupSerial" : 0,
      "Message" : "Infusing the power of enchantments from \"Force\"\nand applying it to weapons—\nthat's another significant role of the Shield\nSage.",
      "MsgSerial" : 212539,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : 0
