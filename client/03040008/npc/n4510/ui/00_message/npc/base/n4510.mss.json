      "DispType" : 0,
      "GmdIndex" : 25,
      "JumpGroupSerial" : 0,
      "Message" : "If you want to improve as a soldier,\nthe best way would be actual combat.\nGrasp what flows on the battlefield.",
      "MsgSerial" : 399119,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
