      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 24,
      "JumpGroupSerial" : 0,
      "Message" : "――It seems that I have obtained a disciple\nwho greatly exceeds my initial expectations.\nI must also express my gratitude to Razanailt.",
      "MsgSerial" : 543317,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
    "NameDispOff" : false,
    "NpcId" : 4510,
    "NpcName" : "Adair Donnchadh"
