      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 20,
      "JumpGroupSerial" : 0,
      "Message" : "You may not become master them immediately,\nbut if you continue to train as you have before,\nsoon your skills will become second nature to you.",
      "MsgSerial" : 543314,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
