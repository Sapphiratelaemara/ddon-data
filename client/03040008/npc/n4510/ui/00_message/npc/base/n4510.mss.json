      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 16,
      "JumpGroupSerial" : 0,
      "Message" : "I see well your efforts.\nI am willing to offer you my expertise,\nbut first, it is important to uphold\nyour obligation to Razanailt.",
      "MsgSerial" : 543312,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
