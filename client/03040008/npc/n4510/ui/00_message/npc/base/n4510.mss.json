    "GroupTypeName" : "MSG_GROUP_TYPE_REV_CHECK_REMAIN",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 21,
      "JumpGroupSerial" : 0,
      "Message" : "Hmmm - I didn't expect that a foreigner\nwould wield the Spirit Lance so well...!",
      "MsgSerial" : 420261,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 22,
      "JumpGroupSerial" : 0,
      "Message" : "Your noble demeanor, it truly embodies \nthe grandeur of a distinguished Spirit Lancer. \nIt is truly remarkable how you have transformed.",
      "MsgSerial" : 543315,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 23,
      "JumpGroupSerial" : 0,
      "Message" : "Your strong, clear eyes are a testament\nto your potential to grasp\neven more advanced techniques. \nSeize the opportunity to further enhance\nyour abilities and unlock even greater blessings.",
      "MsgSerial" : 543316,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    }, {
