      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 17,
      "JumpGroupSerial" : 0,
      "Message" : "It was likely Razanailt who first recognized\nyour potential and took an interest in you. \nGo and report back to her.",
      "MsgSerial" : 543313,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
