  "IndexNum" : 0,
  "Indices" : [ {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おまえたちがアッカーシェランから戻って\nどれくらい経ったろうな",
    "MessageIndex" : 0
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "有意義な時を過ごしていたことだろうが\nどうやら、それはもう終わりのようだ",
    "MessageIndex" : 1
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "白竜がおまえを待っている\n深刻な話らしい\nしっかりと聞いて、あとで教えてくれ",
    "MessageIndex" : 2
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "もし事が起これば神殿として行動せねば\nならないこともあろう",
    "MessageIndex" : 3
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "それを思うと気が重いが――\nはて、今度はいったい何が起こっているのやら",
    "MessageIndex" : 4
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "なんだろうね\n僕は覚者じゃないけれど\nここ最近の白竜様からは、感じ取れる物があるんだ",
    "MessageIndex" : 5
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "心のざわめきとでもいうのかな\nそして、今感じているざわめきは\nあまり良い兆しとは言えないんだ",
    "MessageIndex" : 6
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "我が後継者よ――",
    "MessageIndex" : 7
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "今、理がひとつ壊されようとしている",
    "MessageIndex" : 8
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "我とともに――\nおまえ自身の竜の目で見るのだ",
    "MessageIndex" : 9
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "早くフィンダムに行った方がいい",
    "MessageIndex" : 10
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あそこは新しい精霊竜セシリーが\nまだ眠り続けている",
    "MessageIndex" : 11
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "つまり彼らは自分たちで身を守るしかないんだ\nさぞかし不安だろうからね",
    "MessageIndex" : 12
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "我が後継者よ\nフィンダムへ行け",
    "MessageIndex" : 13
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "フィンダムを滅ぼしてはならぬ\n精霊竜に受けた恩義を今こそ返すのだ",
    "MessageIndex" : 14
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "準備はどうだ、隊長\n俺たちはいつでもいいぜ",
    "MessageIndex" : 15
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "黒い渦ってなんだろうね？\n近くまで行けば黒竜の狙いが\nわかるかな？",
    "MessageIndex" : 16
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "レオ統率の名前が出ていたようですが――",
    "MessageIndex" : 17
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "誰かがどこかで会ったんですか？\n俺も久しぶりに会いたいですよ",
    "MessageIndex" : 18
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "わたしはこのレスタニアに黒竜の手が及ぶ\nその兆しを見逃さぬよう目を光らせよう",
    "MessageIndex" : 19
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "覚者、騎士団、区別なく動いてもらうつもりだ\nおまえたちは急ぎフィンダムへ向かってくれ",
    "MessageIndex" : 20
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "縁浅からぬおまえが駆け付ければ\n彼らも安心だろうからな",
    "MessageIndex" : 21
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "覚者さま、よくぞ来てくださいましたね\n族長ガラダより、言伝を預かっております",
    "MessageIndex" : 22
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "今、ガラダはあの禍々しい渦の真下――",
    "MessageIndex" : 23
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "キンガル峡谷の南に位置する\nシェドレアン大神殿の前で、あなたを\nお待ちしています",
    "MessageIndex" : 24
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "恐ろしい予感が胸の奥で暴れるのです\nあの渦は、この世界にあってはならないものです",
    "MessageIndex" : 25
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おいおい、こいつは洒落になんねーぞ",
    "MessageIndex" : 26
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ガラダから、フィンダムの状況を聞いたんなら\nさっさと動こうぜ",
    "MessageIndex" : 27
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ねえ隊長――\nこのあたりの魔物なんだけどさ",
    "MessageIndex" : 28
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "すでに黒い渦に、毒されているみたい！",
    "MessageIndex" : 29
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "この渦が放つ闇の波動\nこれほどの凄まじい力、俺は見たことがありません",
    "MessageIndex" : 30
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "こんなことができるなんて――\n我々の理屈は通用しないのかもしれませんね",
    "MessageIndex" : 31
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "よくぞ来てくれたレスタニアの覚者よ",
    "MessageIndex" : 32
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "見ての通りだ\nフィンダムの空に、突如として巨大な黒い渦が出現し\nシェドレアン大神殿は闇に閉ざされてしまった",
    "MessageIndex" : 33
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "と、同時に黒い渦が巻き起こす禍々しい\n闇の波動に感化され、魔物までが暴れ出している",
    "MessageIndex" : 34
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "アデルドナハと戦士達が、暴れ出す魔物をやみくもに\n倒してきたが、状況は変わらない",
    "MessageIndex" : 35
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "それどころか渦は徐々に勢いを増し\nしきりに大陸の竜力を吸い上げている",
    "MessageIndex" : 36
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "このままでは、大地は枯れ果て\nフィンダムは黒い渦に呑み込まれて\n滅んでしまうだろう",
    "MessageIndex" : 37
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "斥候からの話によれば\n大地の竜力が強く吸い上げられている場所が\nいくつか存在するようだ",
    "MessageIndex" : 38
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "恐らくだが――\nその場所には、竜力を空へと吸い上げる\n何かがあるに違いない",
    "MessageIndex" : 39
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "既にモードレッドがその場所の一つに向かった\nお願いだ\n共に力を合わせ、空の渦を退けて欲しい",
    "MessageIndex" : 40
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "このようなやり方で大地と竜を\n滅ぼしにかかるとは――\n闇の力はそれほどまでに大きいのか",
    "MessageIndex" : 41
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おお、来てくれたな、礼を言おう",
    "MessageIndex" : 42
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ガラダの推測は当たっていたようだ\nここから崖を登った先の広場で、大地に突き立て\nられた異様な黒い剣を見つけたのだ",
    "MessageIndex" : 43
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あろうことかその黒い剣は、大地の竜力を吸い上げ\n空の渦へと送っている",
    "MessageIndex" : 44
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "黒い剣――\nあの黒騎士の剣と見て間違いない",
    "MessageIndex" : 45
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "我々も覚悟を決めて黒剣に近づこうとしているが\nいかんせん力不足\n百戦錬磨の覚者たちよ、どうか力を貸してくれ",
    "MessageIndex" : 46
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "これで結果が出せなければフィンダムは\n終わるだろう",
    "MessageIndex" : 47
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "フィンダムの恩人よ\n大地の竜力が吸い上げられていると思わしき場所には\n魔物が集まっている\n気をつけてくれ！",
    "MessageIndex" : 48
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あんな恐ろしい魔物ははじめてだ\nああ――アデルドナハ達が心配だ",
    "MessageIndex" : 49
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "よくぞ黒剣を破壊してくれた！",
    "MessageIndex" : 50
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "大地の竜力が強く吸い上げられている場所は\n他にも複数確認されている",
    "MessageIndex" : 51
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "そこにも必ず黒剣があると見て間違いないだろう",
    "MessageIndex" : 52
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "竜力が強く吸い上げられている場所は\n闇の波動の影響下にあり\n周辺には、闇に誘われた魔物が多数目撃されている",
    "MessageIndex" : 53
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "さて、次に向かう場所だが――",
    "MessageIndex" : 54
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "そう、地図のこのあたりだ\n既に仲間が戦っているはずだ、助けてやってくれ",
    "MessageIndex" : 55
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "次の竜力が吸い上げられている場所は\n地図のこのあたりだ",
    "MessageIndex" : 56
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "仲間を助けてやってくれ",
    "MessageIndex" : 57
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "来てくれたか\n黒剣は見つけたが、中には魔物がいる！",
    "MessageIndex" : 58
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "頼む、力を貸してくれ！",
    "MessageIndex" : 59
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "見事だ、覚者殿！\nこの場所での竜力の吸い上げは止んだようだ",
    "MessageIndex" : 60
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "残す黒剣の場所を探るため\n始まりの洞へ向かい、仲間達の消息を追おう",
    "MessageIndex" : 61
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "無事に、二つ目の黒剣を見つけ出し\n破壊してくれたようだな",
    "MessageIndex" : 62
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "これで、残された場所はひとつとなった\n最後の場所はこの先だ",
    "MessageIndex" : 63
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "竜力の吸い上げを阻止しようと\nアデルドナハ達が向かっている\nあいつらが無事に帰ってこられるよう力を貸してくれ",
    "MessageIndex" : 64
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "残る竜力の吸い上げが確認された場所は\nこの先だ",
    "MessageIndex" : 65
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
