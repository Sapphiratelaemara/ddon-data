    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 107,
    "Message" : "おお、戻ったな\r\nそれで、向こうはどうだった？"
