    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 151,
    "Message" : "よし、あともう少しだ"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 152,
    "Message" : "お！\r\nそいつは《生乾きの布》じゃないか！"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 153,
    "Message" : "さっそくそいつを渡してくれ！\r\n――キミ、よくこれを平気な顔して持てるね！"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 154,
    "Message" : "よしよし、これをこうして――\r\nふふふ――やっぱりボクは天才だね！"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 155,
    "Message" : "実にすばらしい仕上がり――\r\n１つめのテントの細工は完了だ！"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 156,
    "Message" : "お！\r\nそいつは《オオワラカミツキ》じゃないか！"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 157,
    "Message" : "さっそくそいつを渡してくれ！\r\n――ちょっとちょっと！\r\nその持ち方で渡されたら\r\nまずボクが危ないじゃないか！"
