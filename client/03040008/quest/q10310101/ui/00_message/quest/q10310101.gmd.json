    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 8,
    "Message" : "《集会場》を攻略する"
