    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 57,
    "Message" : "隊長、こんなボクに\r\n最後に何か言葉をかけておくれよ"
