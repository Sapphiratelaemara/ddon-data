    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ちょっと見せてもらえるかな？\r\n――ふむふむ",
    "MessageIndex" : 77
