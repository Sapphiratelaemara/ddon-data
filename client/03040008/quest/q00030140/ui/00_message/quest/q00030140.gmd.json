    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "黒騎士が現れたと聞いたぞ？\n奴め、生きていたのか？",
    "MessageIndex" : 100
