    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あ、あんたたちは一体誰なんだ？",
