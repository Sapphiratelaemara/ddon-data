      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 3,
      "JumpGroupSerial" : 0,
      "Message" : "その竜力を守ろうとしたのか\nあるいは利用しようとしたのかは\nわしにはわかりませぬが――",
      "MsgSerial" : 630922,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
