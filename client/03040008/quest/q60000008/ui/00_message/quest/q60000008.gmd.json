    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 12,
    "Message" : "取りつけたマジッククラッカーを攻撃するの！足元にしがみつけば狙いやすいわ"
