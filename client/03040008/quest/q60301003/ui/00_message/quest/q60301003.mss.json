      "DispType" : 0,
      "GmdIndex" : 24,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "I am certain that Ser Arisen's Pawns \nfeels the same.",
        "Jp" : "覚者様のポーンもそう思っていることでしょう"
      },
      "MsgSerial" : 712939,
