  "IndexNum" : 0,
  "Indices" : [ {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "さあ、アッカーシェランへ急ぐべきでは\nないか？",
    "MessageIndex" : 0
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ネド王子がどういう人物か知らなくても\n解放軍が活気づいたことは敵にも伝わる",
    "MessageIndex" : 1
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "流れに乗り遅れないように――\nいや、流れを生み出すくらいの勢いで\n丁度良いかもしれないぞ",
    "MessageIndex" : 2
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "悪しき竜の思惑はまだわからん\nくれぐれも用心するのだぞ",
    "MessageIndex" : 3
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "戦いのことは素人だけどね――\n防戦一方じゃ気持ちがふさぐと思う",
    "MessageIndex" : 4
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "砦なんか、壁に囲まれているから\nなおさらだ",
    "MessageIndex" : 5
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "各地から報告が入っています\n状況はますます厳しくなってきたようです",
    "MessageIndex" : 6
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "詳しいことは将軍からお願いします",
    "MessageIndex" : 7
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "敵は同時にあちこちで動き出している\nどこから手をつけたらいいのか――",
    "MessageIndex" : 8
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "増強されたと言ってもまだまだ\n多勢に無勢",
    "MessageIndex" : 9
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "兵を効率的に動かさないと\n勝利が遠のく",
    "MessageIndex" : 10
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ジタバタしても仕方がねえ\nメイロワ将軍と話してこいよ",
    "MessageIndex" : 11
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おそらく、部隊を分散することに\nなるんでしょうね\nその時は、僕にも考えがあります",
    "MessageIndex" : 12
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ネド王子の面倒はわたしに任せて",
    "MessageIndex" : 13
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "この気持ち、なんだろうね\nお姉さん気分――え？　母親気分！？",
    "MessageIndex" : 14
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おお、おぬしら、状況は切迫しておるぞ\nロスギルの村、ベルタのアジトともに敵の攻撃を\n受けている",
    "MessageIndex" : 15
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "やつらめ、本腰を入れて攻めて来たようだ",
    "MessageIndex" : 16
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ネド王子の合流で士気は高まったが\nこのまま劣勢になれば元も子もない",
    "MessageIndex" : 17
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "起死回生――派手な戦果を上げて\n士気を維持するのだ",
    "MessageIndex" : 18
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "派手な戦果を求めるとなれば\nネド王子の出陣は必須",
    "MessageIndex" : 19
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "危険ではあるがこれも王家の責務\n武運は王子にあると信じて\n前進あるのみだ",
    "MessageIndex" : 20
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あなたはロスギルへ行ってくれる？\nわたしはここでもう少し報告を聞いて\n行き先を決めるから",
    "MessageIndex" : 21
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ベルタのところか、それとも、他のところか",
    "MessageIndex" : 22
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "とにかく、ロスギルはあなたに任せるから\nお願いね",
    "MessageIndex" : 23
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ロスギルは任せるから――\n急いでね",
    "MessageIndex" : 24
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "僕はベルタのところへ行こうと思います",
    "MessageIndex" : 25
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あそこには子供が多いから――\n子供たちなら、僕でも勇気づけて\nやれるはずです",
    "MessageIndex" : 26
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "この砦の留守はまかせてくれ",
    "MessageIndex" : 27
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ロスギルへ行くんだろ？\nだったら、急ごうぜ",
    "MessageIndex" : 28
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "隊長、行ってきて\n王子はわたしが守るから",
    "MessageIndex" : 29
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ロスギル村ですよね？\n何度か行き来した村ですけど\n間違えたら大変です",
    "MessageIndex" : 30
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "地図で確認しながら行きましょう",
    "MessageIndex" : 31
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おお、ありがたい！\nなんと心強い顔ぶれでしょうか",
    "MessageIndex" : 32
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ここまでは手勢で持ちこたえていますが\nあらたな敵の情報が入りましてな――",
    "MessageIndex" : 33
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "わたしたちはここの防衛で手一杯\nどうぞ、外の敵の相手をよろしくお願いします！",
    "MessageIndex" : 34
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "敵の援軍は村の外です\n中に入られては守り切れるかどうか――\nどうぞ、よろしくお願いします！",
    "MessageIndex" : 35
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "故郷は自分たちで守る――\nそう言いたいところだが多勢に無勢\n応援、感謝するよ",
    "MessageIndex" : 36
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おう、俺とエリオットでロスギルを襲撃する\n敵を防いでやるよ",
    "MessageIndex" : 37
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "それ以外は頼んだぜ",
    "MessageIndex" : 38
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "隊長、さっさと片付けましょう\nネド王子が心配ですから\nいつでも向こうに行けるように！",
    "MessageIndex" : 39
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "倒したオークが捨て台詞を残しました――\nほどなくベルタも生きちゃいまい\nオークからは逃げられない",
    "MessageIndex" : 40
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "もちろん、王子も――",
    "MessageIndex" : 41
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "隊長、ここはもう我々で対処可能です\nどうぞ、ベルタのところへ！\n王子をお守りください",
    "MessageIndex" : 42
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ここはもう大丈夫です\nどうぞ、盗賊のアジトへ――王子のところへ！",
    "MessageIndex" : 43
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おかげで助かったよ\nあんた、やっぱりすごいな",
    "MessageIndex" : 44
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "話なら、あっちでしようぜ\n王子と盗賊どもに何かあったら\n俺たちが責められそうな勢いだ",
    "MessageIndex" : 45
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "王子の護衛が何人いるかなんて\n隊長も聞いてないですよね",
    "MessageIndex" : 46
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "リズだけってことはないですよねえ\n不安です、早く行きましょう！",
    "MessageIndex" : 47
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "何か――予感がするんです",
    "MessageIndex" : 48
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "吉兆か凶兆かわからないけど――\nいいえ、その両方が混じり合ったような――",
    "MessageIndex" : 49
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "こりゃあ、あれだ\n嵐の前の静けさってやつだな",
    "MessageIndex" : 50
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "油断するなよ",
    "MessageIndex" : 51
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "隊長――\nやっぱり顔見るとホッとするよ",
    "MessageIndex" : 52
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ベルタはちょっと機嫌が悪いから\n気をつけてね",
    "MessageIndex" : 53
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ここは、戦いには不向きですね",
    "MessageIndex" : 54
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "攻め込まれたら、逃げ場がないですよ",
    "MessageIndex" : 55
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "メイロワのじじいめ、ネド王子まで\n寄越すなんて何を考えているんだい！？",
    "MessageIndex" : 56
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "希望だ、旗印だ、と言われて懸命だけど\nまだ子供じゃないか",
    "MessageIndex" : 57
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "――まあ、それはともかく、とりあえず\nこの急場をどう凌ぐかだよ",
    "MessageIndex" : 58
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "一番重要なことはね、ここで戦えば\nこっちに勝ち目はないってことだ",
    "MessageIndex" : 59
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "その代わり、ほら――",
    "MessageIndex" : 60
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "迎え撃つにはうってつけの場所がある\n地図に印をつけてやるから行ってみな",
    "MessageIndex" : 61
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "手下たちを走らせてあるから、そいつが\n案内するよ",
    "MessageIndex" : 62
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "地図を見せてください\n――ああ、やっぱりそうですね",
    "MessageIndex" : 63
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "オークは洞窟の裏手の、地形が入り組んだ\n山の中を進軍してくるはずです",
    "MessageIndex" : 64
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "谷間の道は狭くて大軍には不向き\nこの地の利を活用しなさい――",
    "MessageIndex" : 65
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "それがベルタの考えだと思います",
    "MessageIndex" : 66
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "さあ、隊長さん、行きましょう",
    "MessageIndex" : 67
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "なんだい――",
    "MessageIndex" : 68
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "王子は見違えたようだね\nでも、まだまだ子供なんだからね\nそれを忘れたら後悔するよ",
    "MessageIndex" : 69
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "谷間の様子を見に行きませんか？\n早い方がいいと思うんです",
    "MessageIndex" : 70
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "なるほど――谷底のオークどもを\n山の上から奇襲するってわけだ",
