      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 7,
      "JumpGroupSerial" : 0,
      "Message" : "ただし、経験値とは勝手が違うところが\nあるから注意も必要だ",
      "MsgSerial" : 536174,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
