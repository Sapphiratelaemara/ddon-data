  "indexNum" : 0,
  "indices" : [ {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "火垂れ山の入口は強固な門で封じられていたと\r\n聞いたが――",
    "MessageIndex" : 0
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "向こうの連中には開門のための方策が\r\nあるのだろうか",
    "MessageIndex" : 1
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ネド王子に振り回されている\r\nこの状況――",
    "MessageIndex" : 2
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "そろそろ大人の手に主導権を取り戻さねば\r\n収拾がつかんぞ",
    "MessageIndex" : 3
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "火垂れ山への入山を拒む門か――\r\n王家の者以外は開門不可能などということは\r\nないだろうな",
    "MessageIndex" : 4
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ネド王子は言ってみれば\r\n王家にだけ許された特権を惜しげも無く使って\r\n大人たちを翻弄しているわけだよね",
    "MessageIndex" : 5
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "これって身の危険だけではなくて\r\n王子の地位まで危うくするんじゃないかな",
    "MessageIndex" : 6
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "何も考えていないのか、それほどの危険を\r\n冒すほどの危機意識があるのか――\r\nうーん、僕には判断できないよ",
    "MessageIndex" : 7
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ジリアンから事情は聞かせて貰っている",
    "MessageIndex" : 8
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ここは火垂れ山の入り口にあたる場所\r\n山を登れば、竜の住み処だ",
    "MessageIndex" : 9
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おまえたちがここまでの道を切り開いてくれた\r\nお陰で、この火垂れ山の門前の地を\r\n取り戻すことができた",
    "MessageIndex" : 10
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "だが――\r\nあの門を見て分かるように",
    "MessageIndex" : 11
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "門は、王家の秘蹟でしか開くことができない",
    "MessageIndex" : 12
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "つまり、ネド王子しか\r\n開くことはできないということになる",
    "MessageIndex" : 13
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ふん、そこで俺がこの地を守る、門番のバシアスに\r\n頼まれ力になりに来たという訳さ",
    "MessageIndex" : 14
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "古くから、俺たち盗賊はある竜力の秘められた\r\n指輪の存在を知っていた",
    "MessageIndex" : 15
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "それは、アッカー王家の開祖である\r\nサマラ・アッカーの指輪のことだ\r\nその指輪があれば、扉は開くとされている",
    "MessageIndex" : 16
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "今、指輪がどこにあるのか――",
    "MessageIndex" : 17
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "その手掛かりはメガドにある\r\nジリアンが既にメガドへ向かった",
    "MessageIndex" : 18
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おまえもメガドで待つジリアンの元へ\r\n向かうと良いだろう",
    "MessageIndex" : 19
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "サマラ・アッカーの指輪は古くから\r\n冒険者や盗賊が探し回っていたんだ\r\nしかしまだ見つかってはいない",
    "MessageIndex" : 20
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "だが、その手掛かりがメガドにあることまでは\r\n探れている",
    "MessageIndex" : 21
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "上手く手掛かりを紐解けば、必ず見つかると\r\n信じているよ",
    "MessageIndex" : 22
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "火垂れ山へ入るための門を開くには、王家の\r\n竜力が必要です",
    "MessageIndex" : 23
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "その手掛かりを知り得る人物は\r\nこの地の情報に精通しているセロしかいないでしょう",
    "MessageIndex" : 24
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "エリオットの傷はどう？\r\n――そう、それならほっとした\r\nそれで、セロと会ってきたのかな？",
    "MessageIndex" : 25
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "こっちは１歩前進\r\n彼の教えてくれた場所で、ちょっとした情報が\r\n手に入ったんだ",
    "MessageIndex" : 26
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ある冒険者の記録でね\r\nその人は600年くらい前のメガドに\r\n住んでいたそうで――",
    "MessageIndex" : 27
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "自分の屋敷の壁に冒険の記録を\r\n彫り付けていたんだって\r\n屋敷は今でいう旧市街にあったの",
    "MessageIndex" : 28
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ううん、わたしもまだ見ていない\r\n案内をしてくれる人がまだ来なくて――",
    "MessageIndex" : 29
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "わたしは案内の人を待つから\r\n隊長は先に行って探してみる？\r\n旧市街の屋敷だよ",
    "MessageIndex" : 30
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "サマラの指輪の行方がわかる有力情報が\r\n手に入るかもしれないんだ",
    "MessageIndex" : 31
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "旧市街にある600年前の冒険者の屋敷だよ\r\n良かったら先に行ってみて",
    "MessageIndex" : 32
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "壁に文字が刻まれている――\r\n“高貴なるサマラの指に輝きし光\r\n我が心を魅了し、支配する光\r\n輝きから逃れんと塔に身を隠すが\r\n我が思いはひとときも離れず”",
    "MessageIndex" : 33
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "床に文字が刻まれている――\r\n“探究と秘密を愛する者よ\r\nここより北の洞窟で待つ\r\n高貴なるサマラの指に輝きし光\r\n人の目に余る煌めきサマラに帰るべし”",
    "MessageIndex" : 34
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "石棺に文字が刻まれている――\r\n“我が欲望は光に焼かれて人外となる\r\n探究と秘密を愛する者よ、どうか我が身に平穏を\r\n高貴なるサマラの指に輝きし光\r\n人の目に余る煌めきサマラに帰るべし”",
    "MessageIndex" : 35
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "サマラの指輪探しは慎重にやらないと\r\n探す人に呪いが襲いかかってしまう――\r\nそんな言い伝えがあるらしいんだ",
    "MessageIndex" : 36
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "あの屋敷の主も一度は指輪を\r\n手に入れたけど、その後に行方が\r\nわからなくなったんだってさ",
    "MessageIndex" : 37
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "え！？\r\n隊長、それはもしかして――サマラの指輪！",
    "MessageIndex" : 38
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "ああ、そうか！\r\n指輪は手にする人の欲望を感じ取って\r\n戒めを与えるそうだから、つまり隊長には\r\nそんな邪心はないってことだね",
    "MessageIndex" : 39
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "その指輪、隊長が大丈夫でもわたしはダメかも？",
    "MessageIndex" : 40
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "他の人も耐えられないかもしれないから\r\nとにかく早く火垂れ山のセロのところへ！",
    "MessageIndex" : 41
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おお、まさしくそれはサマラの指輪だ！",
    "MessageIndex" : 42
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おっと、そうだ、そこへ置いてくれ\r\n指輪は直接バシアスへ渡すとしよう",
    "MessageIndex" : 43
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おまえは何ともないようだが――\r\nサマラの指輪は人を惑わす力を有するという",
    "MessageIndex" : 44
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "この時代にこうして指輪が姿を現したのも\r\nもはや偶然ではないだろう\r\n早速バシアスに、扉を開く準備に\r\nかかって貰うとしよう",
    "MessageIndex" : 45
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "それまでは、しっかりと体の疲れを癒しておくと良い",
    "MessageIndex" : 46
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "サマラの指輪は、それを所有せし者を\r\n見定めると言われている",
    "MessageIndex" : 47
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "いにしえからの火竜の意志が\r\n指輪には、きっとあるに違いない――",
    "MessageIndex" : 48
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "完全なる開門には今少し時間が必要です\r\nまた心苦しいが開門の儀を\r\n皆さんにお見せするわけにはいきません",
    "MessageIndex" : 49
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "どうか今少しお時間をください",
    "MessageIndex" : 50
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "へえ――\r\n６００年も昔の指輪が門の鍵代わりとはね",
    "MessageIndex" : 51
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "どんな意図で作られた指輪なのか――\r\nどんな魔法がかけられているのか――\r\n指輪を求めた者の顛末が悲劇的だけど\r\nなんとも魅惑的な話だね",
    "MessageIndex" : 52
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "おいおい、また待ちぼうけか？",
    "MessageIndex" : 53
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "なんともまた焦らせてくれるじゃないか\r\nしかし物は考えようだ",
    "MessageIndex" : 54
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "時間があればエリオットの傷も\r\nそれだけ癒える\r\nおまえもよく休んでおくのだぞ",
    "MessageIndex" : 55
  } ],
  "keyBufferSize" : 0,
