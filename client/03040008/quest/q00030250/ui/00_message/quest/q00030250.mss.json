      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 6,
      "JumpGroupSerial" : 0,
      "Message" : "これって身の危険だけではなくて\n王子の地位まで危うくするんじゃないかな",
      "MsgSerial" : 726729,
      "MsgType" : 0,
      "SetMotion" : 0,
