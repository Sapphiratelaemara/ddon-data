      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 10,
      "JumpGroupSerial" : 0,
      "Message" : "おまえたちがここまでの道を切り開いてくれた\nお陰で、この火垂れ山の門前の地を\n取り戻すことができた",
      "MsgSerial" : 726732,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
