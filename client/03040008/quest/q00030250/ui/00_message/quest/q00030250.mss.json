      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 19,
      "JumpGroupSerial" : 0,
      "Message" : "おまえもメガドで待つジリアンの元へ\n向かうと良いだろう",
      "MsgSerial" : 726741,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
    } ],
