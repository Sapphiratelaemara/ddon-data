      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 11,
      "JumpGroupSerial" : 0,
      "Message" : "だが――\nあの門を見て分かるように",
      "MsgSerial" : 726733,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
