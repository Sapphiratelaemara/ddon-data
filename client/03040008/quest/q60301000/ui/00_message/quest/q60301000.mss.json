      "GmdIndex" : 8,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "Well, well, Arisen.\nWhat brings you here in such a hurry?",
        "Jp" : "おやおや、覚者さん\nずいぶん急いでどうなさいました\n――本日はどのようなご用件で？"
      },
      "MsgSerial" : 679426,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
