      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 8,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "I want you to go through there to\n《Zoma Village》 in eastern Zandora.\nIt's a remote village far from this temple.",
