      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 9,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "So, you're going to visit the Area Master\nof Eastern Zandora, a woman named Nadia.",
