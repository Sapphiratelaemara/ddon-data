    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "KeyOffset" : 0,
    "LinkOffset" : 0,
    "Message" : "なんとか迂回して\r\nあの門に近づければいいのだが――",
    "MessageIndex" : 66
