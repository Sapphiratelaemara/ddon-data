    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 34,
    "Message" : "アレはすでに見つかったし――\r\nうんうん、そうすれば\r\nあそこまで行けるな――それから"
  }, {
    "Index" : 0,
    "KeyCrcHashDouble" : 0,
