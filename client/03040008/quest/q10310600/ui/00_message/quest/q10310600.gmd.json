    "Index" : 0,
    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 45,
    "Message" : "しかも何かとてつもなく大事なことだったような――\r\nうーん、思い出せないな"
