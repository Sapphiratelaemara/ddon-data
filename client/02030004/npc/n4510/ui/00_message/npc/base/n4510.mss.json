    "GroupType" : 22,
    "GroupTypeName" : "MSG_GROUP_TYPE_REV_1TIME_1DAY",
    "MsgData" : [ {
      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 19,
      "JumpGroupSerial" : 0,
      "Message" : "ふむ、良い鍛練を積んでいるようだな\r\nその面構え――そろそろ新たな技を\r\n身に付ける頃合いのようだ",
      "MsgSerial" : 420260,
      "MsgType" : 0,
      "SetMotion" : 0,
