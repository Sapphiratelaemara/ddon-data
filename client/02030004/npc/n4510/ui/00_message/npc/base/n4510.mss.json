      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 21,
      "JumpGroupSerial" : 0,
      "Message" : "ふむ――異国の者がこれほどまでに\r\n精霊槍を操るようになるとは――！",
      "MsgSerial" : 420261,
      "MsgType" : 0,
      "SetMotion" : 0,
      "TalkFaceType" : 0,
      "VoiceReqNo" : -1
