      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 20,
      "JumpGroupSerial" : 0,
      "Message" : "すぐに己がものとはならないだろうが\r\nこれまでと同じく鍛練に励めば\r\nじきに貴様の腕に馴染もう",
      "MsgSerial" : 543314,
      "MsgType" : 0,
