      "@class" : "org.sehkah.ddon.tools.extractor.common.logic.resource.entity.ui.MsgData",
      "DispTime" : 0,
      "DispType" : 0,
      "GmdIndex" : 22,
      "JumpGroupSerial" : 0,
      "Message" : "貴様がまとう風格、立派な\r\n《スピリットランサー》そのものだ\r\n実に――見違えるものだな",
      "MsgSerial" : 543315,
      "MsgType" : 0,
      "SetMotion" : 0,
