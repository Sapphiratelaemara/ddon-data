    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 8,
    "Message" : "リベンジゲージは左上の体力ゲージ下にあるから蓄積量を確認しろ！"
