    "KeyCrcHashDouble" : 0,
    "KeyCrcHashTriple" : 0,
    "LinkOffset" : 0,
    "KeyOffset" : 0,
    "MessageIndex" : 52,
    "Message" : "あんた、届けてあげてよ\r\nメイリーフは商店区にいるから"
